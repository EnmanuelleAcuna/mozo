@using SGJD_INA.Models.ViewModels

@model DetalleAyudaUsuarioViewModel

@{
    Layout = null;
}

<div class="modal-content">
    <div class="row">
        <div class="col s12">
            <h6 class="center text-uppercase blue-text font-bold">
                Ayuda en vista
            </h6>
        </div>

        <div class="col s12">
            <p>
                @Html.Raw(Model.MensajeAyuda)
            </p>
        </div>

        <div class="modal-footer text-center">
            <a class="modal-close waves-effect btn-small grey lighten-3 red-text font-bold">Cerrar</a>
        </div>
    </div>
</div>



