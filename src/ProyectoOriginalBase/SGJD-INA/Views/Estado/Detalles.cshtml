@using SGJD_INA.Models.ViewModels

@model DetallesEstadoViewModel

@{
    Layout = null;
}

<h6 class="center text-uppercase">
    Detalles del estado
</h6>

<p>
   <span class="font-bold">@Html.DisplayNameFor(model => model.Nombre):</span>
    @Html.DisplayFor(model => model.Nombre)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.Descripcion):</span>
    @Html.DisplayFor(model => model.Descripcion)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.CodigoEstado):</span>
    @Html.DisplayFor(model => model.CodigoEstado)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.TipoObjeto):</span>
    @Html.DisplayFor(model => model.TipoObjeto)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.Aviso):</span>
    @Html.DisplayFor(model => model.Aviso)
</p>

<div class="center">
    <button type="button" title="Cerrar" value="Cerrar" class="btn btn-small grey lighten-3 red-text font-bold waves-effect fz-0-8r" onclick="CerrarDialog()">
        Cerrar
    </button>
</div>
