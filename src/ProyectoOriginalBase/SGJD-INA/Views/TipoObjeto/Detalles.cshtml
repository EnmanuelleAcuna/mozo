@using SGJD_INA.Models.ViewModels

@model DetallesTipoObjetoViewModel

@{
    Layout = null;
}

<h6 class="center text-uppercase">
    Detalles del Tipo de Objeto
</h6>

<p class="center font-bold">
    @Html.DisplayFor(model => model.Descripcion)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.NombreTabla):</span>
    @Html.DisplayFor(model => model.NombreTabla)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.Consecutivo):</span>
    @Html.DisplayFor(model => model.Consecutivo)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.Nomenclatura):</span>
    @Html.DisplayFor(model => model.Nomenclatura)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.ParametroEdicion):</span>
    @Html.DisplayFor(model => model.ParametroEdicion)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.ParametroEncabezadoPie):</span>
    @Html.DisplayFor(model => model.ParametroEncabezadoPie)
</p>

<div class="center">
    <button type="button" title="Cerrar" value="Cerrar" class="btn btn-small grey lighten-3 red-text font-bold waves-effect fz-0-8r" onclick="CerrarDialog()">
        Cerrar
    </button>
</div>