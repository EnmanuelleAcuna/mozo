@using SGJD_INA.Models.ViewModels

@model DetalleUsuarioViewModel

@{
    Layout = null;
}

<h6 class="center text-uppercase">
    Detalles del usuario
</h6>

<p class="center font-bold">
    @Html.DisplayFor(model => model.Nombre)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.Identificacion):</span>
    @Html.DisplayFor(model => model.Identificacion)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.Nombre):</span>
    @Html.DisplayFor(model => model.Nombre)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.Rol):</span>
    @Html.DisplayFor(model => model.Rol)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.Unidad):</span>
    @Html.DisplayFor(model => model.Unidad)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.TipoUsuario):</span>
    @Html.DisplayFor(model => model.TipoUsuario)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.Correo):</span>
    @Html.DisplayFor(model => model.Correo)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.NombreUsuario):</span>
    @Html.DisplayFor(model => model.NombreUsuario)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.Estado):</span>
    @Html.DisplayFor(model => model.Estado)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.UltimaConexion):</span>
    @Html.DisplayFor(model => model.UltimaConexion)
</p>

<div class="center">
    <button type="button" title="Cerrar" value="Cerrar" class="btn btn-small grey lighten-3 red-text font-bold waves-effect fz-0-8r" onclick="CerrarDialog()">
        Cerrar
    </button>
</div>