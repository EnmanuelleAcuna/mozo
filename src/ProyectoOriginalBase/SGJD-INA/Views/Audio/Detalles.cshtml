@using SGJD_INA.Models.ViewModels

@model TranscripcionAudioViewModel

@{
    Layout = null;
}

<h6 class="center text-uppercase">
    Transcripción del audio
</h6>

<p class="center font-bold">
    @Html.DisplayFor(model => model.Nombre)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.Transcripcion):</span>
    @Html.DisplayFor(model => model.Transcripcion)
</p>

<div class="center">
    <button type="button" title="Cerrar" value="Cerrar" class="btn btn-small grey lighten-3 red-text font-bold waves-effect fz-0-8r" onclick="CerrarDialog()">
        Cerrar
    </button>
</div>