@using SGJD_INA.Models.ViewModels

@model DetalleSesionViewModel

@{
    Layout = null;
}

<h6 class="center text-uppercase font-bold fz-1r black-text">
    Detalles de sesión
</h6>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.TipoSesion):</span>
    @Html.DisplayFor(model => model.TipoSesion)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.NumeroSesion):</span>
    @Html.DisplayFor(model => model.NumeroSesion)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.FechaSesion):</span>
    @Html.DisplayFor(model => model.FechaSesion)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.NombreEstado):</span>
    @Html.DisplayFor(model => model.NombreEstado)
</p>

<p>
    <span class="font-bold">@Html.DisplayNameFor(model => model.NombreUsuario):</span>
    @Html.DisplayFor(model => model.NombreUsuario)
</p>

<div class="center">
    <button type="button" title="Cerrar" value="Cerrar" class="btn btn-small grey lighten-3 red-text waves-effect fz-0-8r" onclick="CerrarDialog()">
        Cerrar
    </button>
</div>

